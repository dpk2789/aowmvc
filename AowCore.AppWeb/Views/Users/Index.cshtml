@model IEnumerable<AowCore.AppWeb.ViewModels.ApplicationUserViewModel>

<div class="table-responsive">
    <table class="table" id="reportTable" style="width:100%">
        <thead>
            <tr>
             
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
              
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    
                  
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                   
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id" class="fa fa-edit">Edit</a> |
                        @*<a asp-action="Details" asp-route-id="@item.Id">Details</a> |*@
                        <a asp-action="Delete" asp-route-id="@item.Id" class="fa fa-trash">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>